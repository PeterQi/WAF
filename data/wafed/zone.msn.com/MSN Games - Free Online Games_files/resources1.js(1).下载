// global JScript Variables for the Zone10 Site
function clsZHeadNav(Txt, Lnk, Intgid, Alt, HotUrl)
{
this.Txt = Txt;
this.Lnk = Lnk;

// append intgid appropriately
if (Intgid.length > 0)
{
    if (this.Lnk.indexOf("?") == -1)
    {
        this.Lnk += "?" + Intgid;
    }
    else
    {
        this.Lnk += "&" + Intgid;
    }
}

this.Alt = Alt;
this.HotUrl = HotUrl;
}

