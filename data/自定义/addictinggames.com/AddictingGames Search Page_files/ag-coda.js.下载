btg.ComScore = function(a) {
	if (!btg.Controller.hasReporting()) return;
	this.btgIsStr = btg.String.isDefined;
	this.config = a;
	this.c1 = this.btgIsStr(this.config.c1) ? this.config.c1 : "2";
